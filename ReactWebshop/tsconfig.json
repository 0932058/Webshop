{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es6", //es5 last used
    "jsx": "react",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "types": ["webpack-env"],
    "strictNullChecks": false,
    "noImplicitAny": false

  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
